package com.zj198.action.loan;

import com.zj198.action.BaseAction;

public class LoanCalculatorAction extends BaseAction {
	private Long edLastSum;
	private Long edTimes;
	private Double edRate;
	private Long ddlPayWay;
	private Long rbPayFreq;
	public String execute() {
		return SUCCESS;
	}
	
	//setter and getter
	public Long getEdLastSum() {
		return edLastSum;
	}
	public void setEdLastSum(Long edLastSum) {
		this.edLastSum = edLastSum;
	}
	public Long getEdTimes() {
		return edTimes;
	}
	public void setEdTimes(Long edTimes) {
		this.edTimes = edTimes;
	}
	public Double getEdRate() {
		return edRate;
	}
	public void setEdRate(Double edRate) {
		this.edRate = edRate;
	}
	public Long getDdlPayWay() {
		return ddlPayWay;
	}
	public void setDdlPayWay(Long ddlPayWay) {
		this.ddlPayWay = ddlPayWay;
	}
	public Long getRbPayFreq() {
		return rbPayFreq;
	}
	public void setRbPayFreq(Long rbPayFreq) {
		this.rbPayFreq = rbPayFreq;
	}
}
