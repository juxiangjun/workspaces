<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
    <package name="partner" namespace="/pnr" extends="base">
		<action name="pnrResource" class="com.zj198.action.partner.PartnerResourceAction">
		    <result name="user">/WEB-INF/web/user/partner/userSearch.jsp</result>
		    <result name="apply">/WEB-INF/web/user/partner/applySearch.jsp</result>
		    <result name="fee">/WEB-INF/web/user/partner/feeSearch.jsp</result>
		    <result name="login">/public/partner/login.jsp</result>
		</action>
		<action name="pnrLogin" class="com.zj198.action.partner.PartnerLoginAction">
		    <result name="success" type="redirect">/pnr/pnrResource!findUser.act</result>
		    <result name="login">/public/partner/login.jsp</result>
		</action>
	</package>
</struts>