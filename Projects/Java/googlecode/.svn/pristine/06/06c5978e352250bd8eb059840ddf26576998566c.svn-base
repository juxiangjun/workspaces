<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
<package name="json-default-base" extends="base">
        <result-types>
            <result-type name="json" class="org.apache.struts2.json.JSONResult"/>
        </result-types>
        <interceptors>
            <interceptor name="json" class="org.apache.struts2.json.JSONInterceptor"/>
        </interceptors>
    </package>

	<package name="loan" namespace="/loan" extends="base">
		<action name="LoanRequest" class="com.zj198.action.loan.LoanRequestAction" >
			<result name="busiFirst">/public/loan/busiloan_request_first.jsp</result>
			<result name="loanSecond">/public/loan/busiloan_request_second.jsp</result>
			<result name="loanThird">/public/loan/busiloan_request_third.jsp</result>
			<result name="financing">/public/loan/financing_Service.jsp</result>
			<!-- begin个人综合消费快速申请(add by zeroleavebaoyang@gmail.com) -->
			<result name="percostFirst">/public/loan/percost_request_first.jsp</result>
			<!-- end个人综合消费快速申请(add by zeroleavebaoyang@gmail.com) -->
		
			<!-- begin个人经营贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
			<result name="perrunFirst">/public/loan/perrun_request_first.jsp</result>
			<!-- end个人经营贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
		
			<!-- begin个人购房贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
			<result name="perhouseFirst">/public/loan/perhouse_request_first.jsp</result>
			<!-- end个人购房贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
			
			<!-- begin融资需求管理(add by zeroleavebaoyang@gmail.com) -->
			<result name="loanManage">/WEB-INF/web/user/loan/loanManage.jsp</result>
			<result name="loanManageDetail">/WEB-INF/web/user/loan/loanManage_detail.jsp</result>
			<result name="index" type="redirect">/Account.act</result>
			<!-- end融资需求管理(add by zeroleavebaoyang@gmail.com) -->
		</action>
		
	</package>
	<package name="loanadmin" namespace="/loanadmin" extends="base">
		<action name="financeApply" class="com.zj198.action.loan.FinanceApplyAction">
		</action>
		<action name="financeProduct" class="com.zj198.action.loan.FinanceProductAction">
			<result name="new_finanace_page">/WEB-INF/web/user/loan/finance_product_new.jsp</result>
			<result name="view_finance">/WEB-INF/web/user/loan/finance_product_view.jsp</result>
			<result name="financeSearch">/WEB-INF/web/user/loan/finance_product_search.jsp</result>
			<result name="financeSearchDetail">/WEB-INF/web/user/loan/finance_product_searchDetail.jsp</result>
		</action>		
	</package>
	<package name="json" namespace="/json" extends="json-default-base">
		<action name="validateLogin" class="com.zj198.action.loan.LoanRequestAction"  method="validateLogin">
			<result name="ajax" type="json"></result>
		</action>
	</package>
</struts>