<?xml version="1.0" encoding="UTF-8"?>
     <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="DataSource" 
		class="org.apache.commons.dbcp.BasicDataSource">
		<property name="driverClassName"
			value="com.mysql.jdbc.Driver">
		</property> 
		<property name="url"
			value="jdbc:mysql://127.0.0.1:3306/ebs">
		</property>
		<property name="username" value="ebs"></property>
		<property name="password" value="ebs"></property>
	</bean>
	
	<bean id="SessionFactory"
		class="org.springframework.orm.hibernate4.LocalSessionFactoryBean">
		<property name="dataSource">
			<ref bean="DataSource" />
		</property>
		<property name="hibernateProperties">
			<props>
				<prop key="hibernate.dialect">
					org.hibernate.dialect.MySQLDialect
				</prop>
				<prop key="hibernate.show_sql">true</prop>
				<prop key="hibernate.query.factory_class"> 
				    <!--
				    	org.hibernate.hql.classic.ClassicQueryTranslatorFactory
				    	is only for Hibernate 2.x
				    -->
				    org.hibernate.hql.ast.ASTQueryTranslatorFactory 
				</prop>
			</props>
		</property>
		<property name="hibernate.search.default.directory_provider" value="filesystem"/> 
		<property name="hibernate.search.default.indexBase" value="/tmp/ebs/lucene/indexes"/> 
		<property name="annotatedClasses">
			<list>
			<value>org.tju.ebs.bean.OrgOccupation</value>
			<value>org.tju.ebs.bean.SysView</value>
			<value>
				org.tju.ebs.bean.OrgEmployeeDepartment
			</value>
			<value>org.tju.ebs.bean.SysRegistration</value>
			<value>org.tju.ebs.bean.SysProvince</value>
			<value>org.tju.ebs.bean.OrgPosition</value>
			<value>org.tju.ebs.bean.SysObject</value>
			<value>org.tju.ebs.bean.Address</value>
			<value>
				org.tju.ebs.bean.SysObjectRelated
			</value>
			<value>org.tju.ebs.bean.Contact</value>
			<value>org.tju.ebs.bean.OrgCompany</value>
			<value>org.tju.ebs.bean.SysMenu</value>
			<value>org.tju.ebs.bean.OrgDepartment</value>
			<value>org.tju.ebs.bean.SysModel</value>
			<value>org.tju.ebs.bean.OrgCompanyParam</value>
			<value>org.tju.ebs.bean.OrgCompanyModel</value>
			<value>org.tju.ebs.bean.OrgBank</value>
			<value>org.tju.ebs.bean.SysGroup</value>
			<value>
				org.tju.ebs.bean.SysCurrencyExchange
			</value>
			<value>
				org.tju.ebs.bean.SysObjectProperty
			</value>
			<value>org.tju.ebs.bean.SysUser</value>
			<value>
				org.tju.ebs.bean.ModelTransaction
			</value>
			<value>org.tju.ebs.bean.SysGroupMenu</value>
			<value>org.tju.ebs.bean.OrgEmployee</value>
			<value>org.tju.ebs.bean.SysCountry</value>
			<value>org.tju.ebs.bean.SysCity</value>
			<value>org.tju.ebs.bean.SysGroupUser</value>
			<value>org.tju.ebs.bean.SysHelp</value>
			<value>org.tju.ebs.bean.SysViewDetail</value>
			<value>
				org.tju.ebs.bean.OrgEmployeePosition
			</value>
			<value>
				org.tju.ebs.bean.OrgCompanyAddress
			</value>
			<value>org.tju.ebs.bean.SysCurrency</value>
			<value>org.tju.ebs.bean.OrgPerson</value>
			<value>
				org.tju.ebs.bean.OrgEmployeeOccupation
			</value>
			<value>org.tju.ebs.bean.SysParam</value>
			<value>org.tju.ebs.bean.ProdBrand</value>
			<value>org.tju.ebs.bean.ProdBrand</value>
			<value>org.tju.ebs.bean.ProdItem</value>
