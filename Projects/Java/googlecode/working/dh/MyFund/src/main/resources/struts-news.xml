<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="news" namespace="/news" extends="base">
		<action name="helpCenter" class="com.zj198.action.news.HelpCenterAction">
    	   <result name="center">/public/information_centre.jsp</result>
    	</action>
    	<action name="information" class="com.zj198.action.news.NewsAction">
    	   <result name="government">/public/news/government.jsp</result>
    	   <result name="financeInformation">/public/news/financeInformation.jsp</result>
    	   <result name="zjTrends">/public/news/zjTrends.jsp</result>
    	</action>
    	<action name="government" class="com.zj198.action.news.GovernmentAction">
    	   <result name="center">/public/news/help/helpCenter.jsp</result>
    	</action>
    	<action name="finance" class="com.zj198.action.news.FinanceInformationAction">
    	   <result name="center">/public/news/help/helpCenter.jsp</result>
    	</action>
    	<action name="zj" class="com.zj198.action.news.FinanceInformationAction">
    	   <result name="center">/public/news/help/helpCenter.jsp</result>
    	</action>
    	
    	<action name="knwType" class="com.zj198.action.news.KnwTypeAction">
    		<result name="success">/public/news/knwTypeHome.jsp</result>
    		<result name="edit">/public/news/editKnwType.jsp</result>
    	</action>
    	<action name="knwTitle" class="com.zj198.action.news.KnwTitleAction">
    		<result name="success">/public/news/knwTitleHome.jsp</result>
    		<result name="edit">/public/news/editKnwTitle.jsp</result>
    	</action>
    	<action name="knwContent" class="com.zj198.action.news.KnwContentAction">
            <result name="success">/public/news/knwContentHome.jsp</result>
            <result name="edit">/public/news/editKnwContent.jsp</result>
        </action>
            
	</package>
</struts>